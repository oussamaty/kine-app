{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "./src/*"
      ],
      "assets/*": [
        "./src/assets/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "config/*": [
        "./src/config/*"
      ],
      "constants/*": [
        "./src/constants/*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "navigation/*": [
        "./src/navigation/*"
      ],
      "redux/*": [
        "./src/redux/*"
      ],
      "screens/*": [
        "./src/screens/*"
      ],
      "services/*": [
        "./src/services/*"
      ],
      "theme/*": [
        "./src/theme/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ]
    }
  },
  "include": [
    "src",
    "declaration.d.ts",
  ],
}